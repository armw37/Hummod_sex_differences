  <repeat><reps> 100 </reps><stepsize>  1 </stepsize></repeat>
</repeatlist>

<slidebar>
  <row> 12.6 </row><col> 1.0 </col>
  <name> Tau </name>
  <listname> Tau </listname>
  <label> Tau (Hrs) </label>
</slidebar>

<showvalue>
   <row> 14.0 </row><col> 3.0 </col>
