    <name> System.X </name>
    <label> Time </label>
    <scale><min> 0 </min><max> 2 </max></scale>
  </xaxis>
  <yaxis>
    <yvar>
      <name> Rate </name>
      <nolabel/>
      <linecolor> BLUE </linecolor>
    </yvar>
    <scale><min> 0 </min><max> 1 </max></scale>
  </yaxis>
</showgraph>

<showvalue>
   <row> 10.0 </row><col> 1.0 </col>
   <name> RightpO2Effect </name>
   <format><decimal> 2 </decimal></format>
   <label> Right pO2 Effect </label>
</showvalue>

<showvalue>
   <row> 11.0 </row><col> 1.0 </col>
   <name> LeftpO2Effect </name>
   <format><decimal> 2 </decimal></format>
   <label> Left pO2 Effect </label>
</showvalue>

<repeatlist>
