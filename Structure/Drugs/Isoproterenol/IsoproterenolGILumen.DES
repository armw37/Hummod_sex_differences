<structure><name> IsoproterenolGILumen </name>

<variables>

<constant>
  <name> BIOAVAIL </name>
  <val> 0.25 </val>
</constant>

<constant>
  <name> PERM </name>
  <val> 0.010 </val>
</constant>

<var>
  <name> [Isoproterenol] </name>
</var>

<var>
  <name> Loss </name>
</var>

</variables>

<equations>

<diffeq>
  <name> Mass </name>
  <integralname> Mass </integralname>
  <initialval> 0.0 </initialval>
  <dervname> Change </dervname>
  <errorlim> 0.01 </errorlim>
</diffeq>

</equations>

<definitions>

<block><name> CalcConc </name>

<def>
  <name> [Isoproterenol] </name>
  <val> Mass / GILumenH2O.Mass(L) </val>
</def>

</block>

<block><name> Dervs </name>

<def>
  <name> Loss </name>
  <val> BIOAVAIL * PERM * Mass  </val>
</def>

<def>
  <name> Change </name>
  <val> - Loss / BIOAVAIL </val>
</def>

</block>

</definitions>
</structure>

