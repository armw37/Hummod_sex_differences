<structure><name> KPool </name>

<variables>

<var><name> Gain </name></var>
<var><name> Loss </name></var>

<var><name> [K+] </name></var>
<var><name> [K+]-Initial </name></var>

<var><name> [K+(mEq/L)] </name></var>
<var><name> [K+(mEq/L)-Initial] </name></var>

<var><name> InitialMass </name></var>

</variables>

<equations>

<diffeq>
  <name> Mass </name>
  <integralname> Mass </integralname>
  <dervname> Change </dervname>
  <errorlim> 0.660 </errorlim>
</diffeq>

</equations>

<definitions>

<block><name> Init </name>

<def>
  <name> Mass </name>
  <val> InitialMass </val>
</def>

<def>
  <name> [K+]-Initial </name>
  <val> [K+(mEq/L)-Initial] / 1000 </val>
</def>

</block>

<block><name> Conc </name>
  <def><name> [K+] </name><val> Mass / ECFV.Vol </val></def>
  <def><name> [K+(mEq/L)] </name><val> 1000.0 * [K+] </val></def>
</block>

<block><name> Dervs </name>

<def><name> Gain </name>
  <val>  
      GILumenPotassium.Absorption
    + KFluxToPool.Rate
    + IVDrip.KRate
    + Transfusion.KRate
  </val>
</def>

<def><name> Loss </name>
  <val>  
      CollectingDuct_K.Outflow
    + KFluxToCell.Rate
    + SweatDuct.KOutflow
    + Hemorrhage.KRate
    + DialyzerActivity.K+Flux
  </val>
</def>

<def><name> Change </name><val> Gain - Loss </val></def>

</block>

</definitions>
</structure>

End