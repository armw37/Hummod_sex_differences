<structure><name> BladderUrea </name>
<variables>

<var><name> [Urea] </name></var>
<var><name> [Urea(mG/dL)] </name></var>

<constant><name> InitialConc </name><val> 6.46 </val></constant>
<var><name> InitialMass </name></var>

</variables>

<equations>

<diffeq>
  <name> Mass </name>
  <integralname> Mass </integralname>
  <dervname> Change </dervname>
</diffeq>

</equations>

<definitions>

<block><name> Init </name>

  <def><name> InitialMass </name><val> InitialConc * BladderVolume.InitialVolume </val></def>
  <def><name> Mass </name><val> InitialMass </val></def>

</block>

<block><name> Dervs </name>

  <def><name> Change </name><val> CollectingDuct_Urea.Outflow </val></def>

</block>

<block><name> Conc </name>

  <def><name> [Urea] </name><val> Mass / BladderVolume.Mass </val></def>
  <def><name> [Urea(mG/dL)] </name><val> 100.0 * [Urea] </val></def>

</block>

<block><name> Void </name>

  <def><name> Mass </name><val> Mass - ( BladderVolume.VolumeVoid * [Urea] ) </val></def>

</block>

</definitions>
</structure>

End