<structure><name> RightHeart-Lactate </name>

<variables>

<var><name> [Lac-] </name></var>
<var><name> [Lac-(mEq/L)] </name></var>
<var><name> [Lac-(mG/dL)] </name></var>
<parm><name> DC </name><val> 3.0 </val></parm>
<var><name> Made </name></var>
<var><name> Made(mG/Min) </name></var>
<var><name> Used </name></var>
<var><name> Used(mG/Min) </name></var>

<var><name> Flow </name></var>
<var><name> Volume </name></var>
<var><name> K </name></var>
<var><name> [Pool] </name></var>
<var><name> Outflux </name></var>

<constant><name> MEQ_ML_TO_MG_DL </name><val> 9008.0 </val></constant>
<constant><name> GLUCOSE_TO_LACTATE </name><val> 0.99 </val></constant>
<constant><name> MG_TO_MEQ </name><val> 0.0112 </val></constant>

</variables>

<equations>

<backwardeuler>
  <name> Mass </name>
  <integralname> Mass </integralname>
  <initialval> 0.030 </initialval>
  <f1name> F1 </f1name>
  <f2name> F2 </f2name>
  <dervname> Change </dervname>
  <errorlim> 0.0003 </errorlim>
</backwardeuler>

</equations>

<definitions>
<block><name> CalcConc </name>

<def>
  <name> [Lac-] </name>
  <val> Mass / RightHeart-Size.LiquidVol </val>
</def>

<def>
  <name> [Lac-(mEq/L)] </name>
  <val> 1000.0 * [Lac-] </val>
</def>

<def>
  <name> [Lac-(mG/dL)] </name>
  <val> MEQ_ML_TO_MG_DL * [Lac-] </val>
</def>

</block>

<block><name> CalcDervs </name>

<def>
  <name> Made(mG/Min) </name>
  <val> GLUCOSE_TO_LACTATE * RightHeart-Fuel.AnaerobicGlucoseUsed(mG/Min) </val>
</def>

<def>
  <name> Made </name>
  <val> MG_TO_MEQ * Made(mG/Min) </val>
</def>

<def>
  <name> Used(mG/Min) </name>
  <val> RightHeart-Fuel.LacUsed(mG/Min) </val>
</def>

<def>
  <name> Used </name>
  <val> MG_TO_MEQ * Used(mG/Min) </val>
</def>

<def>
  <name> Flow </name>
  <val> RightHeart-Flow.BloodFlow </val>
</def>

<def>
  <name> Volume </name>
  <val> RightHeart-Size.LiquidVol </val>
</def>

<def>
  <name> K </name>
  <val> ( Flow * DC ) / ( Flow + DC ) </val>
</def>

<def>
  <name> [Pool] </name>
  <val> LacPool.[Lac-] </val>
</def>

<def>
  <name> F1 </name>
  <val> Made - Used + ( K * [Pool] ) </val>
</def>

<def>
  <name> F2 </name>
  <val> K / Volume </val>
</def>

<def>
  <name> Outflux </name>
  <val> K * ( [Lac-] - [Pool] ) </val>
</def>

</block>
</definitions>
</structure>

End