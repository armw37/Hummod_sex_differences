Schema : DES V1.0

<structure><name> LeftMedullaNa </name>

<variables>

<var><name> [Na+] </name></var>
<var><name> [Na+(mEq/L)] </name></var>
<var><name> Osmolarity </name></var>
<var><name> Washup </name></var>

<parm><name> CCEfficiency </name><val> 96.0 </val></parm>
<var><name> CCK </name></var>

</variables>

<equations>

<diffeq>
  <name> Mass </name>
  <integralname> Mass </integralname>
  <initialval> 5.68 </initialval>
  <dervname> Change </dervname>
  <errorlim> 0.15 </errorlim>
</diffeq>

</equations>

<definitions>
<block><name> Parms </name>

<def><name> CCK </name><val> 1.0 - ( CCEfficiency / 100.0 ) </val></def>

</block>

<block><name> CalcConc </name>
<if><test> LeftMedulla.Volume GT 0 </test>
<true>
<def><name> [Na+] </name><val> Mass / LeftMedulla.Volume </val></def>
</true>
<false>
<def><name> [Na+] </name><val> 0</val></def>
</false>
</if>

<def><name> [Na+(mEq/L)] </name><val> 1000.0 * [Na+] </val></def>
<def><name> Osmolarity </name><val> 2.0 * [Na+(mEq/L)] </val></def>

</block>

<block><name> Dervs </name>

<def><name> Washup </name><val> CCK * [Na+] * LeftVasaRecta.Outflow </val></def>
<def><name> Change </name><val> LeftCollectingDuct_Na.Reab - Washup </val></def>

</block>

</definitions>
</structure>

End