<structure><name> ANPPool </name>

<variables>

<var><name> [ANP] </name></var>
<var><name> [ANP(pG/mL)] </name></var>
<var><name> Log10Conc </name></var>
<constant><name> PMOLTOPG </name><val> 3.060 </val></constant>
<var><name> Gain </name></var>
<var><name> Loss </name></var>
<var><name> Change </name></var>

<var><name> Target[ANP] </name><val> 20.0 </val></var>
<parm><name> Target[ANP]Target </name><val> 20.0 </val></parm>

<var><name> InitialMass </name></var>
<var><name> sizeModification </name></var>

<parm><name> Clamp </name><val> FALSE </val></parm>
<parm><name> Level </name><val> 0.0 </val></parm>

</variables>

<equations>

<backwardeuler>
  <name> Mass </name> <!-- pmol-->
  <integralname> Mass </integralname>
  <f1name> F1 </f1name>
  <f2name> F2 </f2name>
  <dervname> Derv </dervname>
  <errorlim> 3.00 </errorlim>
</backwardeuler>

</equations>

<definitions>

<block><name> Init </name>
  <def><name> InitialMass </name><val> Target[ANP]Target * ECFV.InitialVol(L) * sizeModification </val></def>
  <def><name> Mass </name><val> InitialMass </val></def>
</block>

<block><name> Initialize </name>

<def><name> Mass </name><val> InitialConc * ECFV.Vol(L) </val></def>

</block>

<block><name> CalcConc </name>

<conditional>
  <name> [ANP] </name>
  <test> Clamp </test>
  <true> Level </true>
  <false> Mass / ECFV.Vol(L) </false>
</conditional>

<def>
  <name> [ANP(pG/mL)] </name>
  <val> PMOLTOPG * [ANP] </val>
</def>

<conditional>
  <name> Log10Conc </name>
  <test> [ANP] GT 1.0 </test>
  <true> LOG10 ( [ANP] ) </true>
  <false> 0.0 </false>
</conditional>

</block>

<block><name> Dervs </name>

<def>
  <name> Gain </name>
  <val> ANPSecretion.Rate + ANPPump.Rate </val>
</def>

<def>
  <name> Loss </name>
  <val> ANPClearance.Rate </val>
</def>

<def>
  <name> Change </name>
  <val> Gain - Loss </val>
</def>

<def><name> F1 </name><val> Gain </val></def>
<def><name> F2 </name><val> ANPClearance.K </val></def>

</block>

</definitions>
</structure>

End